{
    "semi": true,
    "tabWidth": 2,
    "printWidth": 90,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParent": "avoid",
    "trailingComma": "all",
    "endOfLine": "auto",
    "importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }